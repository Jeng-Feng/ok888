		<!-- dragPhotoHint 拖拉圖片至圖框的提示圖 -->
		<image id="dragPhotoHint" folderUrl="assets/defaultStyle/hints/" filename_TW="dragingDown_tw.png" filename_CN="dragingDown_cn.png" filename_JP="dragingDown_jp.png" filename_EN="dragingDown_en.png" width="300" height="227" loadBeforeRunning="1" />
		
		<!-- deletePageHint 刪除頁面的操作提示圖 -->
		<image id="deletePageHint" folderUrl="assets/defaultStyle/hints/" filename_TW="howToDeletePages_tw.png" filename_CN="howToDeletePages_cn.png" filename_JP="howToDeletePages_jp.png" filename_EN="howToDeletePages_en.png" width="" height="" loadBeforeRunning="0" />
		
		<!-- lowerDpiAlert 影像解析度過小的警告提示圖標 -->
		<image id="lowerDpiAlert" folderUrl="assets/defaultStyle/hints/" filename_TW="lowerDpiAlert_tw.png" filename_CN="lowerDpiAlert_cn.png" filename_JP="lowerDpiAlert_jp.png" filename_EN="lowerDpiAlert_en.png" width="48" height="48" loadBeforeRunning="0" />
		
		<!-- readOnlyHint 當編輯器作為唯讀狀態時的提示圖 -->
		<image id="readOnlyHint" folderUrl="assets/defaultStyle/hints/" filename_TW="readOnlyHint_tw.png" filename_CN="readOnlyHint_cn.png" filename_JP="readOnlyHint_jp.png" filename_EN="readOnlyHint_en.png" width="400" height="240" loadBeforeRunning="1" />
		
		<!-- scrollLeftArrow 在ScrollerBar滑鼠往左移時的提示箭頭 -->
		<image id="scrollLeftArrow" folderUrl="assets/defaultStyle/hints/" filename_TW="scrollLeft.png" filename_CN="scrollLeft.png" filename_JP="scrollLeft.png" filename_EN="scrollLeft.png" width="45" height="45" loadBeforeRunning="0" />
		
		<!-- scrollRightArrow 在ScrollerBar滑鼠往右移時的提示箭頭 -->
		<image id="scrollRightArrow" folderUrl="assets/defaultStyle/hints/" filename_TW="scrollRight.png" filename_CN="scrollRight.png" filename_JP="scrollRight.png" filename_EN="scrollRight.png" width="45" height="45" loadBeforeRunning="0" />
		
		<!-- viewAbleHint 文字即將超出頁面範圍時的提示圖 -->
		<image id="viewAbleHint" folderUrl="assets/defaultStyle/hints/" filename_TW="textAreaRange.png" filename_CN="textAreaRange.png" filename_JP="textAreaRange.png" filename_EN="textAreaRange.png" width="399" height="202" loadBeforeRunning="0" />
		
		<!-- detectedHint 自動編排時臉部辨識選項提示圖 -->
		<image id="detectedHint" folderUrl="assets/defaultStyle/hints/" filename_TW="detectedHint.png" filename_CN="detectedHint.png" filename_JP="detectedHint.png" filename_EN="detectedHint.png" width="400" height="297" loadBeforeRunning="0" />
	
	</tipImages>
